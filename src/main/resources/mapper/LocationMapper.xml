<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kosa.kojaping.mapper.LocationMapper">
    <select id="selectLocationNoByLocationName" resultType="Long" parameterType="String">
        SELECT locationNo FROM location WHERE locationName = #{locationName}
    </select>
</mapper>