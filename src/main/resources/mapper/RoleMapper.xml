<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kosa.kojaping.mapper.RoleMapper">
    <select id="selectRoleNameByRoleNo" parameterType="Long" resultType="String">
        SELECT roleName FROM roles WHERE roleNo = #{roleNo}
    </select>
</mapper>